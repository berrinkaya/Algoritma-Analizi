  8.SORUNUN CEVABIDIR:
  
  public void insert(int value){
      if(heapSize == data.length )
          throw new HeapException ("yeterli alan yok");
      else{
          heapSize++;
          data[heapSize-1] = value ;
          siftUp(heapSize-1);
       }
   }
   
 
 
