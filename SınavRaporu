 3.SORUNUN CEVABIDIR:
 
 insertionSort(array A)
     for i=1 to length[A-1] do
         value=A[i]
         j=i-1
         while j>=0 and A[j]>value
             A[j+1]=A[j]
             j=j-1
         A[j+1]=value
         
         
         
         
 4.SORUNUN CEVABIDIR:
 
 procedure bubbleSort(A) defined as:  #A bir dizidir
 do
     swapped := false
     for each i in 0 to length (A)-2 do:
         if A[i]>A[i+1] then
             swap ( A[i], A[i+1] )
             swapped := true
         end if
     end for
  while swapped
  end procedure
  
  
  5.SORUNUN CEVABIDIR:
  
  long pow(int taban, int us){
      if(us==0) return 1;
      if(us==1) return taban;
      if(isEven(us))
          return pow(taban*taban,us/2);
      else
          return pow(taban*taban,us/2)*taban;
          
          
   
